{
    "manifest_version": 3,
    "name": "OverOne",
    "version": "1.0",
    "description": "Proxy Chrome Extension",
    "action": {
        "default_popup": "./dist/index.html"
    },
    "background": {
        "service_worker": "scripts/background.js",
        "type": "module"
    },
    "minimum_chrome_version": "93",
    "permissions": [
        "proxy",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "scripts/*.js",
                "scripts/content_main.js",
                "gists.js",
                "*.svg"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ]
        }
    ]
}