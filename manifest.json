{
    "manifest_version": 3,
    "name": "OverOne",
    "version": "1.0",
    "description": "Proxy Chrome Extension",
    "action": {
        "default_title": "OverOne",
        "default_icon": {
            "16": "./images/Logo16.png",
            "24": "./images/Logo24.png",
            "32": "./images/Logo32.png"
        },
        "default_popup": "./popup/index.html"
    },
    "background": {
        "service_worker": "scripts/background.js",
        "type": "module"
    },
    "minimum_chrome_version": "93",
    "permissions": [
        "proxy",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}