{
    "manifest_version": 3,
    "name": "MyProxy",
    "version": "1.0",
    "description": "Proxy Chrome Extension",
    "background": {
        "service_worker": "scripts/background.js",
        "type": "module"
    },
    "action": {
        "default_popup": "/popup/index.html"
    },
    "minimum_chrome_version": "92",
    "permissions": [
        "proxy",
        "storage"
    ],
    "host_permissions": ["<all_urls>"]
}