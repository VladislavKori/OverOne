{
    "manifest_version": 3,
    "name": "PROX",
    "version": "1.0",
    "description": "Proxy Chrome Extension",
    "background": {
        "service_worker": "scripts/background.js",
        "type": "module"
    },
    "minimum_chrome_version": "92",
    "permissions": [
        "proxy"
    ],
    "host_permissions": ["<all_urls>"]
}